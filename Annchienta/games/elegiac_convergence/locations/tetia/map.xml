import annchienta
import scene

# Obtain some references
mapManager = annchienta.getMapManager()
videoManager = annchienta.getVideoManager()
sceneManager = scene.getSceneManager()
currentMap = mapManager.getCurrentMap()
this = annchienta.getActiveObject()

sceneManager.fadeOut()

# Set a new position to our object (=the player)
this.setPosition( annchienta.Point( annchienta.TilePoint, 9, 3 ) )

# Remove our object from this map...
currentMap.getCurrentLayer().removeObject( this )

# Load and set the new map.
currentMap.setCurrentLayer(1)

# Put our object in the new map.
currentMap.getCurrentLayer().addEntity( this )
mapManager.updateOnce()

videoManager.begin()
mapManager.renderFrame()

sceneManager.fadeIn()
</area>
